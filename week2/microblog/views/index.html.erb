<head>
</head>
<body>
  <div class="header">
    <h1>
      List all blog posts here.
    </h1>
    <div class="buttons">
      Buttons!
    </div>
  </div>
  <div class="page-content">
    <% @posts.each do |id, post| %>
      <h1><%= post.title %></h1>
      <div class="description">
        <%= post.content %>
      </div>
    <% end %>
  </div>
</body>